<div class="d-flex justify-content-between flex-wrap flex-md-nowrap align-items-center pt-3 pb-2 mb-3 border-bottom">
    <h1 class="h2">Professor</h1>
    <div class="btn-toolbar mb-2 mb-md 0">
        <div class="btn-group me-2">
            <button class="btn btn-sm btn-outline-primary"><a href="/">Dashboard</a></button>
        </div>
    </div>
  </div>
  <form action="/dosen/add" method="POST">
    <div class="row form-group mb-4">
    <div class="col">
        <label for="Name">Name</label>
        <input type="text" class="form-control custom-input" id="Name" name="Name" value="" placeholder="Name" required>
    </div>
    <div class="col">
        <label for="Id">ID</label>
        <input type="text" class="form-control custom-input" id="ID" name="ID" value="" placeholder="ID" required>
    </div>
    </div>
    <div class="row form-group mb-4">
    <div class="col">
        <label for="fakultas">Faculty</label>
        <input type="text" class="form-control custom-input" id="fakultas" name="Fakultas" value="" placeholder="Fakultas" required>
    </div>
    <div class="col">
        <label for="Course">Course</label>
        <input type="text" class="form-control custom-input" id="Course" name="Course" value="" placeholder="Course" required>
    </div>
    </div>
    <div class="row form-group mb-4">
    <div class="col">
        <label for="tel">Telephone Number</label>
        <input type="text" class="form-control custom-input" id="tel" name="tel" value="" placeholder="Telephone" required>
    </div>
    <div class="col">
        <label for="email">Email</label>
        <input type="text" class="form-control custom-input" id="email" name="email" value="" placeholder="Email" required>
    </div>
    </div>
    <div class="form-group mb-4">
        <label for="last_educ">Last Education</label>
        <input class="form-control custom-input" name="last_educ" id="last_educ" placeholder="Last Education"></input>
    </div>
    <div class="form-group mb-4">
        <button type="submit" class="btn btn-outline-success">Add Professor</button>
    </div>
  </form>
  
